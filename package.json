  "main": "n/a",
  "type": "module",
  "scripts": {
    "deploy": "gh-pages -d out --tag v$npm_package_version --nojekyll",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:css": "stylelint '**/*.{css,scss}'",
    "format": "npm run format:prettier && npm run format:eslint && npm run format:stylelint",
    "format:prettier": "prettier --write .",
    "format:eslint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format:stylelint": "stylelint '**/*.{css,scss}' --fix",
    "changeset": "changeset"
  },
  "dependencies": {
    "@mdi/js": "^7.3.67",
    "@mdi/react": "^1.6.1",
    "classcat": "^5.0.4",
    "next": "^14.2.21",
    "ramda": "^0.30.1",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.1",
    "@types/node": "^22.5.2",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.7",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "gh-pages": "^6.1.0",
